	<SequenceMapping keySequence="KEYCODE_C,KEYCODE_H" targetChar="Ц" shiftModifier="true"/>
	<SequenceMapping keySequence="KEYCODE_S,KEYCODE_H" targetChar="Ш" shiftModifier="true"/>
	<SequenceMapping keySequence="KEYCODE_S,KEYCODE_H,KEYCODE_H" targetChar="Щ" shiftModifier="true"/>
	<SequenceMapping keySequence="KEYCODE_Z,KEYCODE_X" targetChar="Ъ" shiftModifier="true"/>
	<SequenceMapping keySequence="KEYCODE_Y,KEYCODE_U" targetChar="Ю" shiftModifier="true"/>
	<SequenceMapping keySequence="KEYCODE_Y,KEYCODE_A" targetChar="Я" shiftModifier="true"/>
</PhysicalTranslation>